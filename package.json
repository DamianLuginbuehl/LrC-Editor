{
  "name": "lrcedit",
  "type": "module",
  "version": "1.0.0",
  "description": "LrC Editor - Lightroom Classig Calalog Manager",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.yourcompany.lrcedit",
    "productName": "LrC Editor",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "icons/icon.icns"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "icons/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "icons/icon.png"
    }
  },
  "dependencies": {
    "electron-store": "^10.0.1",
    "package.json": "^0.0.0"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.7.0"
  }
}